#include<stdio.h>
int main()
{
	int i,n,a[10];
	printf("Enter the no.of elements=");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("Enter an element=");
		scanf("%d",&a[i]);
	}
	printf("\nOdd numbers:\n");
	for(i=0;i<n;i++)
	{
		if(a[i]%2==1)
		{
			printf("%d ",a[i]);
		}
	}
	printf("\nEven numbers:\n");
	for(i=0;i<n;i++)
	{
		if(a[i]%2==0)
		{
			printf("%d ",a[i]);
		}
	}
}
